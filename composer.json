{
    "name": "undf/angularjsbundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle for AngularJs integration" ,
    "keywords": ["angularjs", "symfony", "javascript", "angular"],
    "homepage": "https://github.com/danidelalin/UndfAngularJsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dani Gonzalez",
            "email": "danigonzalezgonzalez@gmail.com",
            "homepage": "https://github.com/danidelalin/UndfAngularJsBundle"
        }
    ],
    "require": {
        "php":                      ">=5.3.1",
        "symfony/assetic-bundle":   ">=2.1.*"
    },
    "replace": {
        "undf/AngularJsBundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Undf\\AngularJsBundle" : "" }
    },
    "target-dir": "Undf/AngularJsBundle"
}
